                //Throw exception
                if ( ( sum_v_test == 0 ) || ( sum_v_proj == 0 ) )
                {
                        if ( sum_v_test == 0 ) analysis_type.a_nnng = false;

                        throw ErrorBadData ( "ErrorBadData: can not compute NNN graph analysis_type, ", "not enough bounded cells." );
